{
  "name": "webpacktemplate",
  "version": "1.0.0",
  "description": "Basic webpack configuration template.",
  "main": "index.js",
  "scripts": {
    "serve": "nodemon --inspect --watch --config --watch src/server src/server/server.js",
    "start": "webpack-dev-server --config=config/webpack.dev.js",
    "prods": "node src/server/server.js",
    "build:devs": "webpack --config=config/webpack.dev.js",
    "build:prod": "webpack --config=config/webpack.prod.js"
  },
  "author": "Mark Manga",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "brotli-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.0",
    "express-static-gzip": "^1.1.3",
    "extract-loader": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.3",
    "markdown-with-front-matter-loader": "^0.1.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4",
    "redux": "^4.0.0",
    "webpack-dev-middleware": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
